{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "nRF52-DK",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/boards/nrf52dk/target/thumbv7em-none-eabi/release/nrf52dk",
            "target": "localhost:2331",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "arm-none-eabi-gdb",
            "autorun": [
                "monitor reset",
                "monitor speed auto",
                "b reset_handler"
            ]
        },
        {
            "name": "MSP-EXP432P401R",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/target/thumbv7em-none-eabi/debug/msp-exp432p401r",
            "target": "localhost:3333",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "arm-none-eabi-gdb",
            "autorun": [
                "monitor reset",
                "monitor speed auto",
                "b reset_handler"
            ]
        }
    ]
}